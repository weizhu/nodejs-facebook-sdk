<body>
  <div id="fb-root"></div>
  <script src="//connect.facebook.net/en_US/all.js"></script>
  <script>
  window.fbAsyncInit = function() {
    var config = <%- JSON.stringify(config) %>;
    FB.init({
      appId: config.facebook.appId,
      channelUrl : '/channel.html', // Channel File
      status: true,
      cookie: true,
      xfbml: true,
    });
  };

  fbAsyncInit();
  </script>
